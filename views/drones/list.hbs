<h2>Available drones</h2>

{{#each dronesArr}}
    <section class="drone-summary">
        <h2>{{this.name}}</h2>
        <p>
            name: {{this.name}} <br />
            propellers: {{this.propellers}} <br />
            maxSpeed: {{this.maxSpeed}} <br />
        </p>
    </section>
{{/each}}